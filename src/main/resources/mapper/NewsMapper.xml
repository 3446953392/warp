<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yupi.wlsdbackend.mapper.NewsMapper">

    <resultMap id="BaseResultMap" type="com.yupi.wlsdbackend.domain.News">
        <id property="newsId" column="news_id" jdbcType="INTEGER"/>
        <result property="newsTitle" column="news_title" jdbcType="VARCHAR"/>
        <result property="newsDescription" column="news_description" jdbcType="VARCHAR"/>
        <result property="newsUrl" column="news_url" jdbcType="VARCHAR"/>
        <result property="newsTag" column="news_tag" jdbcType="VARCHAR"/>
        <result property="newsDate" column="news_date" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        news_id
        ,news_title,news_description,
        news_url,news_tag,news_date
    </sql>
</mapper>
